<h1>Editing ul_file</h1>

<% form_for(@ul_file) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :sender_name %><br />
    <%= f.text_field :sender_name %>
  </p>
  <p>
    <%= f.label :recipient_email %><br />
    <%= f.text_field :recipient_email %>
  </p>
  <p>
    <%= f.label :filename %><br />
    <%= f.text_field :filename %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @ul_file %> |
<%= link_to 'Back', ul_files_path %>